@model List<ENTITIES.Models.Tecrübelerim>
	@{
		ViewData["Title"] = "DeneyimlerList";
		Layout = "~/Views/Layout/Index.cshtml";
	}


		<div class="content">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-12">

						<div class="card">
							<div class="card-header">
								<div class="card-title">Deneyimler</div>
							</div>
							<div class="card-body">
								<table class="table table-hover">
									<thead>
										<tr>
											<th scope="col">Id</th>
											<th scope="col">Tip</th>
											<th scope="col">Başlık</th>
											<th scope="col">Unvan</th>
											<th scope="col">Date</th>
											<th scope="col">Aciklama</th>
											<th scope="col">İşlemler</th>

										</tr>
									</thead>
									<tbody>
										@foreach (var item in Model) {
											<tr>
												<td>@item.Id</td>
												<td>@item.Tip</td>
												<td>@item.Baslik</td>
												<td>@item.Unvan</td>
												<td>@item.Date</td>
												<td>@item.Aciklama</td>
												<td>
											<a asp-action="DeneyimlerGuncelle" asp-route-id="@item.Id">Edit</a> |
											<a asp-action="DeneyimlerSil" asp-route-id="@item.Id">Delete</a>
												</td>
											</tr>
										}
									</tbody>
								</table>
						<a href="/Admin/Deneyimler/DeneyimlerEkle" class="btn btn-primary">Yeni Ekle</a>
							</div>
						</div>

					</div>
				</div>
			</div>
		</div>
